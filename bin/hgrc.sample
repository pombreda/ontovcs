[extensions]
hgext.extdiff =

[extdiff]
cmd.owl2diff = owl2diff

[diff-patterns]
**.owl = owl2merge

[merge-patterns]
**.owl = owl2merge

[merge-tools]
owl2diff.priority=-100
owl2diff.gui=False
owl2diff.diffargs=$child $parent

owl2merge.priority=-100
owl2merge.gui=True
owl2merge.args=$base $local $other -o $output --auto
owl2merge.diffargs=$parent $child
